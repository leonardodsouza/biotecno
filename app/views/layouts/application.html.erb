<!DOCTYPE html>
<html>
  <head>
    <title>Biotecno</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    
    <%= javascript_include_tag 'application' %>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>

    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-112543242-1"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'UA-112543242-1');
    </script>


  </head>

  <body> 
     
    <% if controller.controller_name != 'bemvindo' %>
      <%= render  "layouts/menu" %>
      <%= yield %>
      <%= render  "layouts/footer" %>
    <%else%>  
      <%= yield %>
    <%end%>    

  </body>
</html>
